<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.stream">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="Listen" android:name=".ListenActivity" />
        <activity android:label="Albums" android:name=".AlbumActivity" />
        <activity android:label="Playlists" android:name=".PlaylistActivity" />
        <activity android:label="Tracks" android:name=".TrackActivity"></activity>
    </application>

</manifest>